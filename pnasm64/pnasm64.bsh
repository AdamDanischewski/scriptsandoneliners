alias pnasm64='_(){ local sel="";local file="${1}"; (($#<1))||[[ "${1}" =~ ^- || ! -e "${file}" ]]&&echo "Usage: pnasm <your.c> [-t (compile and link)]"&&return 1;rm -f "${file%.c}.o" "${file%.c}.asm";echo "Decompiling into object code.";gcc -fno-asynchronous-unwind-tables -s -c "${file}" -o "${file%.c}.o";echo "Converting to nasm.";objconv -fnasm "${file%.c}.o"; sed -i "s/align=1//g;s/[a-z]*execute//g;s/: *function//g;/default *rel/d" "${file%.c}.asm";[[ -n "${2}" && "${2}" =~ "-t" ]]&&echo "Compiling with nasm..."&&nasm -felf64 -o "${file%.c}2.o" "${file%.c}.asm"&&echo "Linking with gcc..."&&gcc -no-pie -o "${file%.c}" "${file%.c}2.o";echo "NASM asm code is in: ${file%.c}.asm";};_'
