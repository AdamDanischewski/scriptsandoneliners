alias vc='_(){ local file=${1:-blah};(($#<1))||[[ "${1}" =~ ^- ]]&&echo "Usage: vc <file>"&&return 1;[[ ! -e "${file}" ]]&&echo "File ${file} not found."&&return 1; echo "${file}:";paste <(for a in $(sed -nE "s/(^.*(\b|\x27|\"|:|\(|,))(#|0x)([[:xdigit:]]{6})(\b|\x27|\"|:|\)|,)(.*$)/\4/p" "${file}"); do perl -e "foreach \$a(@ARGV){print \"\e[48;2;\".join(\";\",unpack(\"C*\",pack(\"H*\",\$a))).\"m \e[49m \"};print \"\n\"" "$a"; done) <(sed -nE "s/(^.*(\b|\x27|\"|:|\(|,))(#|0x)([[:xdigit:]]{6})(\b|\x27|\"|:|\)|,)(.*$)/\4/p" "${file}");}; _' 
